# 1. creating datasource section in standalone.xml config
data-source add \
   --jndi-name=java:jboss/datasources/StenusysDemoDS \
   --name=StenusysDemoDS \
   --connection-url=jdbc:postgresql://localhost:5432/java_ee \
   --driver-name=postgresql \
   --user-name=postgres \
   --password=postgres

# 2. restarting Wildfly server
:reload